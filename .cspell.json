{
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    "**/.pnpm-lock.json",
    ".vscode",
    "megalinter",
    "package-lock.json",
    "report",
    "**/*.hbs",
    "**/*.txt"
  ],
  "ignoreRegExpList": [
    "/^\\s*```[\\s\\S]*?^\\s*```/gm",
    "/^\\s*`[\\s\\S]*?^\\s*`/gm"
  ],
  "language": "en-GB",
  "dictionaries": ["en", "en-GB"],
  "noConfigSearch": true,
  "words": [
    "+jolie+",
    "behaviour",
    "behavioural",
    "classpath",
    "classpaths",
    "cset",
    "csets",
    "Dockerfiles",
    "dockerhub",
    "embedder",
    "generalisation",
    "iface",
    "instanceof",
    "JDBC",
    "Jocker",
    "joliedoc",
    "jolielang",
    "joliemock",
    "jolier",
    "jolietraceviewer",
    "keytool",
    "libjolie",
    "localsocket",
    "localsockets",
    "megalinter",
    "Metaservice",
    "Netbeans",
    "openapi",
    "oxsecurity",
    "parameterise",
    "println",
    "servicepath",
    "sodep",
    "sodeps",
    "tquery",
    "univocally",
    "wdsl",
    "XMPP",
    "prestart",
    "fname"
  ],
  "version": "0.2"
}
